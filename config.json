{
  "icons": {
    "src": "_src/assets/icons/*.*",
    "dest": "public/",
    "dist": "public/"
  },
  "html": {
    "src": "_src/templates/*.html",
    "partials": "_src/templates/partials/",
    "dest": "public/",
    "dist": "docs/"
  },
	"scss": {
		"src": "_src/assets/scss/**/*.scss",
		"dest": "public/assets/css/",
    "dist": "docs/assets/css/"
	},
	"js": {
		"src": [
      "_src/assets/js/main.js",
      "_src/assets/js/_personal.js",
      "_src/assets/js/_get-url.js",
      "_src/assets/js/_contact.js",
      "_src/assets/js/_image.js",
      "_src/assets/js/_colors.js",
      "_src/assets/js/_typography.js",
      "_src/assets/js/_checkbox.js",
      "_src/assets/js/_skills.js",
      "_src/assets/js/_checkBox.js"
		],
		"dest": "public/assets/js/",
    "dist": "docs/assets/js/"
	},
	"images": {
    "src": "_src/assets/images/**/*.{gif,jpg,jpeg,JPG,JPEG,png,svg}",
    "dest": "public/assets/images/",
    "dist": "docs/assets/images/"
  },
  "watch": {
   "html": "_src/**/*.html"
  }
}
